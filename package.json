{
  "name": "hello-prisma",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:next && yarn build:nest",
    "build:next": "next build",
    "build:nest": "nest build --path ./tsconfig.server.json",
    "start": "node ./dist/server/main.js",
    "start:next": "next dev",
    "mode:dev": "cross-env NODE_ENV=\"development\"",
    "start:dev": "cross-env NODE_ENV=\"development\" nest start --path ./tsconfig.server.json --watch",
    "start:debug": "nest start --path ./tsconfig.server.json --debug --watch",
    "start:prod": "node dist/server/main",
    "prisma:generate": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:typecheck": "vitest typecheck",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "deploy": "sh ./deploy.sh"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.5.0",
    "@fastify/nextjs": "^10.0.1",
    "@fastify/secure-session": "^7.1.0",
    "@fastify/session": "^10.5.0",
    "@fastify/static": "^6.12.0",
    "@fastify/under-pressure": "^8.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@nest-lab/fastify-multer": "^1.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.2.5",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.11",
    "@prisma/client": "^5.3.0",
    "@react-hook/resize-observer": "^1.2.6",
    "@swc/core": "^1.3.84",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "app-root-path": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "c8": "^8.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "color": "^4.2.3",
    "cross-env": "^7.0.3",
    "nest-next": "^10.1.0",
    "nestjs-sharp": "^1.1.0",
    "nestjs-zod": "^3.0.0",
    "next": "^14.0.4",
    "postcss": "^8.4.32",
    "prisma": "^5.3.0",
    "react": "^18.2.0",
